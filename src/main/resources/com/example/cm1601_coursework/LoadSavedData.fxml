<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="750" prefWidth="900" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.cm1601_coursework.LoadSavedDataController">
    <Button layoutX="682.0" layoutY="687.0" mnemonicParsing="false" onAction="#switchToMenu"
            text="Back to Main Menu">
        <font>
            <Font name="Arial Rounded MT Bold" size="18.0"/>
        </font>
    </Button>
    <Label layoutX="607.0" layoutY="85.0" text="Load Data">
        <font>
            <Font name="Arial Rounded MT Bold" size="24.0"/>
        </font>
    </Label>
    <Label layoutX="413.0" layoutY="175.0" prefHeight="27.0" prefWidth="346.0"
           text="Number of drivers in the latest saved data:">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="40.0" layoutX="754.0" layoutY="169.0" stroke="BLACK"
               strokeType="INSIDE" width="86.0"/>
    <Label fx:id="numberOfDriversLabel" layoutX="761.0" layoutY="175.0" prefHeight="27.0" prefWidth="72.0"
           text="           ">
        <font>
            <Font name="System Bold Italic" size="18.0"/>
        </font>
    </Label>
    <Button fx:id="loadSavedDataButton" layoutX="571.0" layoutY="311.0" mnemonicParsing="false"
            onAction="#loadSavedData" prefHeight="27.0" prefWidth="192.0" text="Load Saved Data">
        <font>
            <Font name="Arial Rounded MT Bold" size="18.0"/>
        </font>
    </Button>
    <Label fx:id="successTextLabel" layoutX="550.0" layoutY="260.0" prefHeight="27.0" prefWidth="290.0"
           text="                                            ">
        <font>
            <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
</AnchorPane>
